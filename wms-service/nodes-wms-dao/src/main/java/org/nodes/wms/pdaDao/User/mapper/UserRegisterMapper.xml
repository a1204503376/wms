<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.nodes.wms.pdaDao.User.mapper.UserRegisterMapper">

    <select id="selectLoginStatusByUserId"
            resultType="org.nodes.wms.pdaDao.User.dto.output.UserLoginStatusResponse">
        SELECT
            login_status,
            last_login_time
        FROM
            log_user_register
        where user_id = #{userId}
        ORDER BY
            last_login_time DESC
        LIMIT 1
    </select>
</mapper>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.nodes.wms.core.common.mapper.AddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="addressResultMap" type="org.nodes.wms.core.common.entity.Address">
        <id column="pa_id" property="paId"/>
        <result column="data_id" property="dataId"/>
        <result column="address_data_type" property="addressDataType"/>
        <result column="address_type" property="addressType"/>
        <result column="address" property="address"/>
